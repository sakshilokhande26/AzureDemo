{
	"name": "Sorting_age",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "blobdata",
						"type": "DatasetReference"
					},
					"name": "blobdata"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "blobtoadls",
						"type": "DatasetReference"
					},
					"name": "target"
				}
			],
			"transformations": [
				{
					"name": "sortage"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id as string,",
				"          name as string,",
				"          age as string,",
				"          city as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> blobdata",
				"blobdata sort(asc(age, true)) ~> sortage",
				"sortage sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> target"
			]
		}
	}
}